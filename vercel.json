{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/frontend.js",
      "dest": "/frontend.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    },
    {
      "src": "/analyze-asset",
      "dest": "api/index.py"
    },
    {
      "src": "/run-agents",
      "dest": "api/index.py"
    },
    {
      "src": "/docs",
      "dest": "api/index.py"
    },
    {
      "src": "/openapi.json",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key"
  }
}
